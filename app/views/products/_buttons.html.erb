  <a href="#myModal" role="button" class="btn btn-info product-chart" data-toggle="modal">View Product Chart</a>
    <div id="myModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">x</button>
        <h4 id="myModalLabel"><%= @product.name %> chart</h4>
      </div>
      <div class="modal-body">
        <%= render 'chart' %>
      </div>
      <div class="modal-footer">
      </div>
    </div>
    <%= link_to 'View Product MSDS', @product.doc.url, class: 'btn btn-info btn-msds', target: "_blank" %>

